{
  "name": "elyerr/{{ module }}",
  "version": "1.0.0",
  "type": "project",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "{{ Namespace }}\\App\\": "app/",
      "{{ Namespace }}\\Database\\Factories\\": "database/factories/",
      "{{ Namespace }}\\Database\\Seeders\\": "database/seeders/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "{{ Namespace }}\\App\\Providers\\ServiceProvider"
      ]
    }
  },
  "require": {
    "elyerr/laravel-runtime": "^1.0",
    "inertiajs/inertia-laravel": "^2.0",
    "laravel/framework": "^12.42",
    "elyerr/api-response": "^1.0"
  },
  "require-dev": {
    "elyerr/oauth2-passport-server": "^5.0"
  },
  "scripts": {
    "post-autoload-dump": [
      "php artisan assets:publish"
    ]
  },
  "minimum-stability": "stable",
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}